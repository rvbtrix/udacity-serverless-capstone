    "patch-package": "^6.2.2",
    "prettier": "^2.2.1",
    "serverless": "^2.55.0",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-iam-roles-per-function": "^3.2.0-e97ab49",
