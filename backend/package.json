{
  "name": "udacity-final-project",
  "version": "1.0.0",
  "description": "Projeto FInal Cloud Develop",
  "scripts": {
    "postinstall": "patch-package",
    "start": "npm-run-all -p offline build:watch",
    "build": "tsc --build .",
    "start:debug": "npm-run-all -p offline:debug build:watch",
    "build:watch": "tsc --build -w .",
    "debug": "cross-env SLS_DEBUG=* && node --inspect=5858 ./node_modules/.bin/serverless offline",
    "offline": "sls offline start --stage local --host 0.0.0.0 --useChildProcesses",
    "offline:debug": "node --inspect ./node_modules/.bin/serverless offline start --stage local --host 0.0.0.0",
    "kill-local-dynamo": "npx fkill -f :8000",
    "test:integration": "mocha --config test/integration/integration.mocharc.yml ./test/integration/**/*.test.ts",
    "test:integration:watch": "npm run test:integration -- --watch-extensions ts --watch -R min",
    "test:unit": "cross-env FORCE_COLOR=1 nyc --silent mocha --config test/unit/unit.mocharc.yml",
    "test:unit:watch": "npm run test:unit -- --watch-extensions ts --watch -R min",
    "test": "npm-run-all test:unit coverage",
    "coverage": "nyc report && nyc check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git"
  },
  "author": "Reginaldo Botelho",
  "private": true,
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chai-subset": "^1.3.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/jws": "^3.2.4",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.2",
    "@types/serverless": "^1.78.25",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "aws-sdk": "^2.884.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
